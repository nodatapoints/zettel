   "source": [
    "import numpy as np\n",
    "\n",
    "phi = (1 + np.sqrt(5))/2\n",
    "psi = 1 - phi"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "mu1 = (5-np.sqrt(5))/10\n",
    "mu2 = 1 - mu1"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
