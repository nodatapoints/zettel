      "9 21 21\n"
     ]
    }
   ],
   "source": [
    "for i in range(1, 10):\n",
    "    v = mu1*phi**i + mu2*psi**i\n",
    "    w = mu1*phi**i - mu2*psi**i\n",
    "    \n",
    "    print(f'{i} {v:.0f} {w:.0f}')"
   ]
  }
